{
  "type": "module",
  "name": "@hazae41/disposable-stack-polyfill",
  "version": "2.0.4",
  "description": "Polyfill for DisposableStack and AsyncDisposableStack",
  "repository": "github:hazae41/disposable-stack-polyfill",
  "author": "hazae41",
  "license": "MIT",
  "sideEffects": true,
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "rm -rf ./out && tsc && tscousin"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@hazae41/phobos": "^2.0.13",
    "@hazae41/tscousin": "^1.0.28",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "polyfill",
    "disposable",
    "stack",
    "disposablestack",
    "using",
    "symbol",
    "dispose",
    "asyncdispose",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
}